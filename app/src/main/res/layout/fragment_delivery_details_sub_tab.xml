<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:cardBackgroundColor="@color/white"
            tools:context=".screens.fragments.DeliveryDetailsSubTabFragment">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/driver_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:text="Driver Name"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/driver_nameTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/driver_name"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen_12" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/driver_nameTV"
                        android:layout_marginVertical="@dimen/dimen5"
                        android:background="@color/hint_text_gray" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/driver_link_iv"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:layout_marginEnd="@dimen/dimen24"
                            android:padding="@dimen/dimen_4"
                            android:src="@drawable/ic_attach_link"
                            android:visibility="gone"
                            app:tint="@color/black" />

                        <ImageView
                            android:id="@+id/driver_whatsapp_iv"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginEnd="@dimen/dimen16"
                            android:layout_toStartOf="@id/driver_call_iv"
                            android:padding="@dimen/dimen3"
                            android:src="@drawable/ic_whatsapp_green_border" />

                        <ImageView
                            android:id="@+id/driver_call_iv"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_centerVertical="true"
                            android:layout_marginEnd="@dimen/dimen16"
                            android:layout_toStartOf="@id/driver_link_iv"
                            android:padding="@dimen/dimen_4"
                            android:src="@drawable/ic_phone_call" />

                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/vehicle_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:text="Vehicle Number"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/vehicle_numberTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/vehicle_number"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen_12" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/vehicle_numberTV"
                        android:layout_marginVertical="@dimen/dimen5"
                        android:background="@color/hint_text_gray" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/lr_noRL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/lr_number"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:text="LR Number"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/lr_numberTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/lr_number"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen_12" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/lr_numberTV"
                        android:layout_marginVertical="@dimen/dimen5"
                        android:background="@color/hint_text_gray" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/mode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:text="Mode of Transport"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/modeTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/mode"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen_12" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/modeTV"
                        android:layout_marginVertical="@dimen/dimen5"
                        android:background="@color/hint_text_gray" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/instruction"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:drawablePadding="@dimen/dimen20"
                        android:drawableTint="@color/orange"
                        android:fontFamily="@font/outfit_medium"
                        android:text="Instruction"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/instructionTV"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/instruction"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen_12" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/instructionTV"
                        android:layout_marginVertical="@dimen/dimen5"
                        android:background="@color/hint_text_gray" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/proof_of_loadingRL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/proof_of_loading"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:text="Proof of Loading"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/loading_select_TV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dimen20"
                        android:layout_toStartOf="@id/upload_loading_proofIV"
                        android:text="select"
                        android:textColor="@color/orange" />

                    <ImageView
                        android:id="@+id/upload_loading_proofIV"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="@dimen/dimen16"
                        android:src="@drawable/ic_upload_curved" />

                    <LinearLayout
                        android:id="@+id/complaint_img_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/proof_of_loading"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/no_proofTV"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginVertical="@dimen/dimen_12"
                            android:background="#F9F9FC"
                            android:fontFamily="@font/outfit_medium"
                            android:paddingVertical="@dimen/dimen20"
                            android:text="No Proof Available"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:visibility="gone" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/upload_loading_file_rv"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dimen120"
                            android:nestedScrollingEnabled="false"
                            android:numColumns="3"
                            tools:listitem="@layout/upload_file_info" />

                        <TextView
                            android:id="@+id/complaint_see_more"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_marginVertical="@dimen/dimen_12"
                            android:fontFamily="@font/outfit_regular"
                            android:text="Show more"
                            android:textColor="@color/orange" />
                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/complaint_img_ll"
                        android:layout_marginVertical="@dimen/dimen5"
                        android:background="@color/hint_text_gray" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/proof_of_deliveryRL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/proof_of_delivery"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/dimen_12"
                        android:fontFamily="@font/outfit_medium"
                        android:text="Proof of Delivery"
                        android:textColor="@color/hint_text_gray"
                        android:textSize="@dimen/dimen_12" />

                    <TextView
                        android:id="@+id/delivery_select_TV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dimen20"
                        android:layout_toStartOf="@id/upload_delivery_proofIV"
                        android:text="select"
                        android:textColor="@color/orange" />

                    <ImageView
                        android:id="@+id/upload_delivery_proofIV"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="@dimen/dimen16"
                        android:src="@drawable/ic_upload_curved" />

                    <LinearLayout
                        android:id="@+id/res_img_ll"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/proof_of_delivery"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/no_proof_of_DeliveryTV"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/proof_of_delivery"
                            android:layout_marginVertical="@dimen/dimen_12"
                            android:background="#F9F9FC"
                            android:fontFamily="@font/outfit_medium"
                            android:paddingVertical="@dimen/dimen20"
                            android:text="No Proof Available"
                            android:textAlignment="center"
                            android:textColor="@color/black"
                            android:visibility="gone" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/upload_delivery_proof_rv"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dimen120"
                            android:layout_below="@id/proof_of_delivery"
                            android:nestedScrollingEnabled="false"
                            android:numColumns="3"
                            tools:listitem="@layout/upload_file_info" />

                        <TextView
                            android:id="@+id/resolve_see_more"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:fontFamily="@font/outfit_regular"
                            android:text="Show more"
                            android:textColor="@color/orange" />
                    </LinearLayout>
                </RelativeLayout>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dimen_12"
                    android:background="@color/white"
                    app:cardCornerRadius="@dimen/dimen5"
                    app:cardElevation="@dimen/dimen5">

                    <LinearLayout
                        android:id="@+id/delivery_detailsLL"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/delivery_detailsRL"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/dimen10"
                                android:layout_marginTop="@dimen/dimen10"
                                android:layout_marginEnd="@dimen/dimen10"
                                android:layout_marginBottom="@dimen/dimen10"
                                android:drawableTint="@color/black"
                                android:fontFamily="@font/outfit_medium"
                                android:text="Delivery Details"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen10" />

                            <ImageView
                                android:id="@+id/arrowIV"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentEnd="true"
                                android:layout_centerVertical="true"
                                android:layout_marginEnd="@dimen/dimen10"
                                android:padding="@dimen/dimen5"
                                android:src="@drawable/ic_right_pointed_arrow"
                                app:tint="@color/black" />
                        </RelativeLayout>
                    </LinearLayout>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/order_statusLL"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimen30"
                        android:background="@color/white"
                        android:visibility="gone"
                        app:cardCornerRadius="@dimen/dimen5">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen10"
                            android:background="@color/white"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/title_text_TV"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/dimen_12"
                                android:fontFamily="@font/outfit_medium"
                                android:text="Order Status"
                                android:textColor="@color/black"
                                android:textSize="@dimen/dimen_12" />

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="@dimen/dimen10"
                                android:background="@color/white"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:id="@+id/accRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:id="@+id/acc_verifiedLL"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_alignParentStart="true"
                                        android:background="@drawable/grass_green_light_gray_circle_bg"
                                        android:padding="@dimen/dimen3"
                                        android:src="@drawable/ic_verified_border"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:tint="@color/green_white_selector" />

                                    <TextView
                                        android:id="@+id/tv_title"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/acc_verifiedLL"
                                        android:fontFamily="@font/outfit_medium"
                                        android:text="Account Not Verified"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/verified_by_TV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/tv_title"
                                        android:layout_marginVertical="@dimen/dimen6"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/acc_verifiedLL"
                                        android:fontFamily="@font/outfit_regular"
                                        android:gravity="center_vertical"
                                        android:textColor="@color/black"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/date_timeTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/verified_by_TV"
                                        android:layout_alignStart="@id/verified_by_TV"
                                        android:layout_marginBottom="@dimen/dimen6"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/poRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/accRL">

                                    <ImageView
                                        android:id="@+id/po_receivedIV"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_alignParentStart="true"
                                        android:layout_marginTop="@dimen/dimen10"
                                        android:background="@drawable/grass_green_light_gray_circle_bg"
                                        android:padding="@dimen/dimen6"
                                        android:src="@drawable/ic_po_received"
                                        android:textColor="@android:color/white"
                                        app:tint="@color/green_white_selector" />

                                    <TextView
                                        android:id="@+id/po_title"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignTop="@id/po_receivedIV"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/po_receivedIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:text="PO not received"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/po_updated_by"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/po_title"
                                        android:layout_marginVertical="@dimen/dimen6"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:fontFamily="@font/outfit_medium"
                                        android:gravity="center_vertical"
                                        android:textColor="@color/black"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/po_date_timeTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/po_updated_by"
                                        android:layout_marginBottom="@dimen/dimen6"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/agentRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/poRL">

                                    <ImageView
                                        android:id="@+id/agent_assignedIV"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_alignParentStart="true"
                                        android:layout_marginTop="@dimen/dimen10"
                                        android:background="@drawable/grass_green_light_gray_circle_bg"
                                        android:padding="@dimen/dimen6"
                                        android:src="@drawable/ic_border_user"
                                        android:textColor="@android:color/white"
                                        app:tint="@color/green_white_selector" />

                                    <TextView
                                        android:id="@+id/agent_titleTV"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignTop="@id/agent_assignedIV"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/agent_assignedIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:text="Agent Not Assigned"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/agent_detailsTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/agent_titleTV"
                                        android:layout_marginVertical="@dimen/dimen6"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/agent_assignedIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:gravity="center_vertical"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/agent_date_timeTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/agent_detailsTV"
                                        android:layout_alignStart="@id/agent_titleTV"
                                        android:layout_marginBottom="@dimen/dimen6"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/loadingRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/agentRL">

                                    <ImageView
                                        android:id="@+id/loading_IV"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_alignParentStart="true"
                                        android:layout_marginTop="@dimen/dimen10"
                                        android:background="@drawable/grass_green_light_gray_circle_bg"
                                        android:padding="@dimen/dimen6"
                                        android:src="@drawable/ic_loading"
                                        android:textColor="@android:color/white"
                                        app:tint="@color/green_white_selector" />

                                    <TextView
                                        android:id="@+id/loading_titleTV"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignTop="@id/loading_IV"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/loading_IV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:text="Loading"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/load_byTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/loading_titleTV"
                                        android:layout_marginVertical="@dimen/dimen6"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/loading_IV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:gravity="center_vertical"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/load_dateTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/load_byTV"
                                        android:layout_alignStart="@id/loading_titleTV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/load_timeTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/load_dateTV"
                                        android:layout_marginBottom="@dimen/dimen6"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/dispatchRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/loadingRL">

                                    <ImageView
                                        android:id="@+id/dispatchIV"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_alignParentStart="true"
                                        android:layout_marginTop="@dimen/dimen10"
                                        android:background="@drawable/grass_green_light_gray_circle_bg"
                                        android:padding="@dimen/dimen6"
                                        android:src="@drawable/ic_truck"
                                        app:tint="@color/green_white_selector" />

                                    <TextView
                                        android:id="@+id/dispatch_titleTV"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignTop="@id/dispatchIV"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/dispatchIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:text="Dispatched"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/dispatch_byTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/dispatch_titleTV"
                                        android:layout_marginVertical="@dimen/dimen6"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/dispatchIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:gravity="center_vertical"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/dispatch_timeTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/dispatch_byTV"
                                        android:layout_alignStart="@id/dispatch_titleTV"
                                        android:layout_marginBottom="@dimen/dimen6"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />
                                </RelativeLayout>

                                <RelativeLayout
                                    android:id="@+id/deliveredRL"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/dispatchRL">

                                    <ImageView
                                        android:id="@+id/deliveredIV"
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        android:layout_alignParentStart="true"
                                        android:layout_marginTop="@dimen/dimen10"
                                        android:background="@drawable/grass_green_light_gray_circle_bg"
                                        android:padding="@dimen/dimen6"
                                        android:src="@drawable/ic_order_delivered"
                                        android:textColor="@android:color/white"
                                        app:tint="@color/green_white_selector" />

                                    <TextView
                                        android:id="@+id/delivered_titleTV"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_alignTop="@id/deliveredIV"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/deliveredIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:text="Delivered"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/delivered_byTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/delivered_titleTV"
                                        android:layout_marginVertical="@dimen/dimen6"
                                        android:layout_marginStart="@dimen/dimen16"
                                        android:layout_toEndOf="@id/deliveredIV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:gravity="center_vertical"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/delivered_dateTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/delivered_byTV"
                                        android:layout_alignStart="@id/delivered_titleTV"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />

                                    <TextView
                                        android:id="@+id/delivered_timeTV"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@id/delivered_dateTV"
                                        android:layout_alignStart="@id/delivered_titleTV"
                                        android:layout_marginBottom="@dimen/dimen6"
                                        android:fontFamily="@font/outfit_medium"
                                        android:maxLines="1"
                                        android:textColor="@color/gray"
                                        android:textSize="10sp" />
                                </RelativeLayout>

                                <LinearLayout
                                    android:layout_width="5dp"
                                    android:layout_height="match_parent"
                                    android:layout_above="@id/deliveredRL"
                                    android:layout_below="@id/accRL"
                                    android:layout_marginStart="13dp"
                                    android:layout_marginTop="-40dp"
                                    android:layout_marginBottom="-20dp"
                                    android:background="@drawable/gray_dash_border"
                                    android:elevation="-2dp"
                                    android:orientation="vertical" />
                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </androidx.core.widget.NestedScrollView>

</layout>